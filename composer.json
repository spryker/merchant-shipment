{
  "name": "spryker/merchant-shipment",
  "type": "library",
  "description": "MerchantShipment module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker-shop/cart-page-extension": "^1.0.0",
    "spryker-shop/checkout-page-extension": "^1.3.0",
    "spryker-shop/customer-page-extension": "^1.3.0",
    "spryker/kernel": "^3.33.0",
    "spryker/sales": "^11.0.0",
    "spryker/shipment": "^8.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
